{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Ext": true
    },
    "parserOptions": {
        "ecmaVersion": 5
    },
    "rules": {
        "indent": [
            "warn",
            4
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "warn",
            "always"
        ],
        "no-mixed-spaces-and-tabs": "warn",
        "no-unused-vars": "warn",
        "no-console": "warn"
    }
}